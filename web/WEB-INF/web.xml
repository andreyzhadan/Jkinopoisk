<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xmlns="http://java.sun.com/xml/ns/javaee"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee
		  http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd"
         version="2.5">
    <display-name>Servlet</display-name>
    <!--<welcome-file-list>-->
    <!--<welcome-file>index.jsp</welcome-file>-->
    <!--</welcome-file-list>-->

    <servlet>
        <servlet-name>userValidator</servlet-name>
        <servlet-class>com.zhadan.UserServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>userValidator</servlet-name>
        <url-pattern>/jkinopoisk</url-pattern>
    </servlet-mapping>

    <filter>
        <filter-name>UserCheckFilter</filter-name>
        <filter-class>com.zhadan.UserCheckFilter</filter-class>
    </filter>
    <filter-mapping>
        <filter-name>UserCheckFilter</filter-name>
        <url-pattern>/jkinopoisk</url-pattern>
    </filter-mapping>
</web-app>
